{
  "name": "@chat/backend-e2e",
  "version": "0.0.1",
  "private": true,
  "nx": {
    "targets": {
      "e2e": {
        "executor": "@nx/jest:jest",
        "outputs": [
          "{workspaceRoot}/coverage/{projectRoot}"
        ],
        "options": {
          "jestConfig": "apps/backend-e2e/jest.config.ts",
          "passWithNoTests": true
        },
        "configurations": {
          "ci": {
            "ci": true,
            "codeCoverage": true
          }
        }
      },
      "lint": {
        "executor": "@nx/eslint:lint",
        "outputs": [
          "{options.outputFile}"
        ],
        "options": {
          "lintFilePatterns": [
            "apps/backend-e2e/**/*.ts"
          ]
        }
      }
    }
  }
}